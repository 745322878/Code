<Window x:Class="SqlCommand实现文件导入数据库.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button  Content="数据录入" Width="200" Height="50" FontSize="20" Click="Button_Click" Margin="0,50"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                <TextBlock Text="TelPhone" FontSize="20" Background="AliceBlue"/>
                <TextBox Width="200" Height="50" x:Name="telphone"/>             
            </StackPanel>
            <Button  Content="号码归属地查询" Width="200" Height="50" FontSize="20" Click="Button_Click_1" Margin="0,50"/>
        </StackPanel>
    </Grid>
</Window>
